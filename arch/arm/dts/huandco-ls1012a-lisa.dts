/dts-v1/;
#include "fsl-ls1012a.dtsi"

/ {
	model = "LISA-LS1012A Board";

	aliases {
		spi0 = &qspi;
	};

	chosen {
		stdout-path = &duart1;
	};
};

&qspi {
	bus-num = <0>;
	status = "okay";

	qflash0: mt25qu128@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-flash";
		spi-max-frequency = <20000000>;
		reg = <0>;
	};
};

&i2c0 {
	status = "okay";
};

&duart1 {
	status = "okay";
};